{
  /*
   * Possible values:
   * - the name of a built-in config
   * - the name of an NPM module which has a "main" file that exports a config object
   * - a relative path to a JSON file
   */
  "rules": {
    "no-unused-variable": false,
    "no-for-in-array": false,
    "restrict-plus-operands": false,
    "missing-jsdoc": false,
    "linebreak-style": [
      false
    ],
    "no-relative-imports": false,
    "member-access": false,
    "react-tsx-curly-spacing": false,
    "max-line-length": [
      150
    ],
    "react-a11y-role-has-required-aria-props": false,
    "react-a11y-event-has-role": false,
    "react-this-binding-issue": false,
    "react-anchor-blank-noopener": false,
    "function-name": false,
    "trailing-comma": [
      true
    ],
    "no-trailing-whitespace": false,
    "no-increment-decrement": false,
    "variable-name": false
  },
  "extends": "tslint-microsoft-contrib"
}